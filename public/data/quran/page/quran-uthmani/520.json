{"pageNumber":520,"ayahs":[{"number":4666,"numberInSurah":36,"text":"وَكَمْ أَهْلَكْنَا قَبْلَهُم مِّن قَرْنٍ هُمْ أَشَدُّ مِنْهُم بَطْشًۭا فَنَقَّبُوا۟ فِى ٱلْبِلَٰدِ هَلْ مِن مَّحِيصٍ","juz":26,"hizbQuarter":208,"page":520,"surah":{"number":50,"name":"سُورَةُ قٓ","englishName":"Qaaf"}},{"number":4667,"numberInSurah":37,"text":"إِنَّ فِى ذَٰلِكَ لَذِكْرَىٰ لِمَن كَانَ لَهُۥ قَلْبٌ أَوْ أَلْقَى ٱلسَّمْعَ وَهُوَ شَهِيدٌۭ","juz":26,"hizbQuarter":208,"page":520,"surah":{"number":50,"name":"سُورَةُ قٓ","englishName":"Qaaf"}},{"number":4668,"numberInSurah":38,"text":"وَلَقَدْ خَلَقْنَا ٱلسَّمَٰوَٰتِ وَٱلْأَرْضَ وَمَا بَيْنَهُمَا فِى سِتَّةِ أَيَّامٍۢ وَمَا مَسَّنَا مِن لُّغُوبٍۢ","juz":26,"hizbQuarter":208,"page":520,"surah":{"number":50,"name":"سُورَةُ قٓ","englishName":"Qaaf"}},{"number":4669,"numberInSurah":39,"text":"فَٱصْبِرْ عَلَىٰ مَا يَقُولُونَ وَسَبِّحْ بِحَمْدِ رَبِّكَ قَبْلَ طُلُوعِ ٱلشَّمْسِ وَقَبْلَ ٱلْغُرُوبِ","juz":26,"hizbQuarter":208,"page":520,"surah":{"number":50,"name":"سُورَةُ قٓ","englishName":"Qaaf"}},{"number":4670,"numberInSurah":40,"text":"وَمِنَ ٱلَّيْلِ فَسَبِّحْهُ وَأَدْبَٰرَ ٱلسُّجُودِ","juz":26,"hizbQuarter":208,"page":520,"surah":{"number":50,"name":"سُورَةُ قٓ","englishName":"Qaaf"}},{"number":4671,"numberInSurah":41,"text":"وَٱسْتَمِعْ يَوْمَ يُنَادِ ٱلْمُنَادِ مِن مَّكَانٍۢ قَرِيبٍۢ","juz":26,"hizbQuarter":208,"page":520,"surah":{"number":50,"name":"سُورَةُ قٓ","englishName":"Qaaf"}},{"number":4672,"numberInSurah":42,"text":"يَوْمَ يَسْمَعُونَ ٱلصَّيْحَةَ بِٱلْحَقِّ ۚ ذَٰلِكَ يَوْمُ ٱلْخُرُوجِ","juz":26,"hizbQuarter":208,"page":520,"surah":{"number":50,"name":"سُورَةُ قٓ","englishName":"Qaaf"}},{"number":4673,"numberInSurah":43,"text":"إِنَّا نَحْنُ نُحْىِۦ وَنُمِيتُ وَإِلَيْنَا ٱلْمَصِيرُ","juz":26,"hizbQuarter":208,"page":520,"surah":{"number":50,"name":"سُورَةُ قٓ","englishName":"Qaaf"}},{"number":4674,"numberInSurah":44,"text":"يَوْمَ تَشَقَّقُ ٱلْأَرْضُ عَنْهُمْ سِرَاعًۭا ۚ ذَٰلِكَ حَشْرٌ عَلَيْنَا يَسِيرٌۭ","juz":26,"hizbQuarter":208,"page":520,"surah":{"number":50,"name":"سُورَةُ قٓ","englishName":"Qaaf"}},{"number":4675,"numberInSurah":45,"text":"نَّحْنُ أَعْلَمُ بِمَا يَقُولُونَ ۖ وَمَآ أَنتَ عَلَيْهِم بِجَبَّارٍۢ ۖ فَذَكِّرْ بِٱلْقُرْءَانِ مَن يَخَافُ وَعِيدِ","juz":26,"hizbQuarter":208,"page":520,"surah":{"number":50,"name":"سُورَةُ قٓ","englishName":"Qaaf"}},{"number":4676,"numberInSurah":1,"text":"بِسْمِ ٱللَّهِ ٱلرَّحْمَٰنِ ٱلرَّحِيمِ وَٱلذَّٰرِيَٰتِ ذَرْوًۭا","juz":26,"hizbQuarter":208,"page":520,"surah":{"number":51,"name":"سُورَةُ الذَّارِيَاتِ","englishName":"Adh-Dhaariyat"}},{"number":4677,"numberInSurah":2,"text":"فَٱلْحَٰمِلَٰتِ وِقْرًۭا","juz":26,"hizbQuarter":208,"page":520,"surah":{"number":51,"name":"سُورَةُ الذَّارِيَاتِ","englishName":"Adh-Dhaariyat"}},{"number":4678,"numberInSurah":3,"text":"فَٱلْجَٰرِيَٰتِ يُسْرًۭا","juz":26,"hizbQuarter":208,"page":520,"surah":{"number":51,"name":"سُورَةُ الذَّارِيَاتِ","englishName":"Adh-Dhaariyat"}},{"number":4679,"numberInSurah":4,"text":"فَٱلْمُقَسِّمَٰتِ أَمْرًا","juz":26,"hizbQuarter":208,"page":520,"surah":{"number":51,"name":"سُورَةُ الذَّارِيَاتِ","englishName":"Adh-Dhaariyat"}},{"number":4680,"numberInSurah":5,"text":"إِنَّمَا تُوعَدُونَ لَصَادِقٌۭ","juz":26,"hizbQuarter":208,"page":520,"surah":{"number":51,"name":"سُورَةُ الذَّارِيَاتِ","englishName":"Adh-Dhaariyat"}},{"number":4681,"numberInSurah":6,"text":"وَإِنَّ ٱلدِّينَ لَوَٰقِعٌۭ","juz":26,"hizbQuarter":208,"page":520,"surah":{"number":51,"name":"سُورَةُ الذَّارِيَاتِ","englishName":"Adh-Dhaariyat"}}],"juz":26,"hizbQuarter":208,"surahs":{"50":{"number":50,"name":"سُورَةُ قٓ","englishName":"Qaaf","englishNameTranslation":"The letter Qaaf","revelationType":"Meccan","numberOfAyahs":45},"51":{"number":51,"name":"سُورَةُ الذَّارِيَاتِ","englishName":"Adh-Dhaariyat","englishNameTranslation":"The Winnowing Winds","revelationType":"Meccan","numberOfAyahs":60}}}